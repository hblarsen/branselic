<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="configurator" nsURI="http://www.itu.dk/branselic" nsPrefix="configurator">
  <eSubpackages name="branselic" nsURI="http://www.itu.dk/configurator/branselic"
      nsPrefix="configurator.branselic">
    <eClassifiers xsi:type="ecore:EClass" name="Feature">
      <eStructuralFeatures xsi:type="ecore:EReference" name="assignment" eType="#//branselic/Assignment"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ConfiguratorModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="feature" lowerBound="1"
          upperBound="-1" eType="#//branselic/Feature" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="configuration" lowerBound="1"
          eType="#//branselic/Configuration" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ruleset" lowerBound="1"
          eType="#//branselic/RuleSet" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="RuleSet">
      <eStructuralFeatures xsi:type="ecore:EReference" name="rule" upperBound="-1"
          eType="#//branselic/Rule" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Rule">
      <eStructuralFeatures xsi:type="ecore:EReference" name="assignment" eType="#//branselic/Assignment"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="booleanexpression" eType="#//branselic/BooleanExpression"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Assignment">
      <eStructuralFeatures xsi:type="ecore:EReference" name="value" eType="#//branselic/Value"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Configuration">
      <eStructuralFeatures xsi:type="ecore:EReference" name="assignment" upperBound="-1"
          eType="#//branselic/Assignment" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Value"/>
    <eClassifiers xsi:type="ecore:EClass" name="Bool" eSuperTypes="#//branselic/Value"/>
    <eClassifiers xsi:type="ecore:EClass" name="Int" eSuperTypes="#//branselic/Value"/>
    <eClassifiers xsi:type="ecore:EClass" name="Enum" eSuperTypes="#//branselic/Value"/>
    <eClassifiers xsi:type="ecore:EClass" name="BooleanExpression"/>
    <eClassifiers xsi:type="ecore:EClass" name="Atom" eSuperTypes="#//branselic/BooleanExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="assignment" eType="#//branselic/Assignment"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Or" eSuperTypes="#//branselic/BooleanExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="booleanexpression" lowerBound="1"
          upperBound="-1" eType="#//branselic/BooleanExpression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Negation" eSuperTypes="#//branselic/BooleanExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="booleanexpression" lowerBound="1"
          upperBound="-1" eType="#//branselic/BooleanExpression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Product">
      <eStructuralFeatures xsi:type="ecore:EReference" name="items" upperBound="-1"
          eType="#//branselic/Items" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Items">
      <eStructuralFeatures xsi:type="ecore:EReference" name="constraint" upperBound="-1"
          eType="#//branselic/Constraint" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Constraint"/>
  </eSubpackages>
</ecore:EPackage>
